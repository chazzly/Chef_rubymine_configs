<templateSet group="ChefSpec">
  <template name="it" value="it '$CURSOR$' do&#10;  expect(chef_run).to $DOSTUFF$&#10;end" toReformat="false" toShortenFQNames="true">
    <variable name="CURSOR" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="DOSTUFF" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="CHEF" value="true" />
    </context>
  </template>
  <template name="describe" value="describe $CURSOR$ do&#10;  let(:chef_run) do&#10;    ChefSpec::SoloRunner.new do |node|&#10;      node.set['etc']['passwd']={}&#10;    end.converge(described_recipe)&#10;  end&#10;  $STUFF$&#10;end " toReformat="false" toShortenFQNames="true">
    <variable name="CURSOR" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="STUFF" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="CHEF" value="true" />
    </context>
  </template>
  <template name="shared_examples" value="shared_examples '$CURSOR$' do&#10;  include_context '$STUFF$'&#10;  $OTHER$&#10;end" toReformat="false" toShortenFQNames="true">
    <variable name="CURSOR" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="STUFF" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="OTHER" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="CHEF" value="true" />
    </context>
  </template>
  <template name="shared_context" value="RSpec.shared_context '$CURSOR' do&#10;  $STUFF$&#10;end" toReformat="false" toShortenFQNames="true">
    <variable name="STUFF" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="CHEF" value="true" />
    </context>
  </template>
  <template name="let" value="let (:$VAR$) do&#10;  $STUFF$&#10;end" toReformat="false" toShortenFQNames="true">
    <variable name="VAR" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="STUFF" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="CHEF" value="true" />
    </context>
  </template>
</templateSet>